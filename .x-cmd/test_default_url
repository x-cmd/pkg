# shellcheck shell=dash

test_default_url_main()(
    git branch --show-current
    x cd .tmp/x-cmd-pkg && \
    xws bundle && rm "$___X_CMD_PKG_RAW_PATH/" && \
    xws test_default_url "$___X_CMD_WEBSRC_REGION" && \
    rsync -acvP doc/ ../../doc/
)

x cd "$(x wsroot)"
xws prepare || return 1
test_default_url_main "${___X_CMD_PKG_VERSION:-v0.0.1}"