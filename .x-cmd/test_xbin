# shellcheck shell=dash

test_xbin_main()(
    x cd .tmp/x-cmd-pkg && \
    xws bundle && rm -rf "$___X_CMD_PKG_RAW_PATH" && \
    xws test_xbin "$___X_CMD_WEBSRC_REGION" && \
    rsync -acvP doc/ ../../doc/
)

x cd "$(x wsroot)"
xws prepare || return 1
test_xbin_main "${___X_CMD_PKG_VERSION:-v0.0.1}"